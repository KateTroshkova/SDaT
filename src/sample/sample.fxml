<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="400.0" type="javafx.scene.layout.BorderPane"
         xmlns="http://javafx.com/javafx/10.0.2-internal">
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
    </padding>
    <top>
        <Pane fx:id="treeField" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        </Pane>
    </top>
    <bottom>
        <VBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <ChoiceBox fx:id="typeBox" prefWidth="150.0"/>
                        <TextField fx:id="urlField" prefHeight="30.0" prefWidth="450.0"/>
                    </children>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="600.0">
                    <children>
                        <Button fx:id="createButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0"
                                text="Создать"/>
                        <Button fx:id="loadButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0"
                                text="Загрузить"/>
                        <Button fx:id="balanceButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0"
                                text="Сбалансировать"/>
                        <Button fx:id="saveButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0"
                                text="Сохранить"/>
                    </children>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <TextField fx:id="addIdField" prefHeight="26.0" prefWidth="100.0"/>
                        <Button fx:id="addIdButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0"
                                text="Добавить"/>
                    </children>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <TextField fx:id="deleteIdField" prefHeight="30.0" prefWidth="100.0"/>
                        <Button fx:id="deleteIdButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0"
                                text="Удалить"/>
                    </children>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <TextField fx:id="getIdField" prefHeight="30.0" prefWidth="100.0"/>
                        <Button fx:id="getIdButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0"
                                text="Найти"/>
                        <TextField fx:id="getResultField"/>
                    </children>
                </HBox>
            </children>
        </VBox>
    </bottom>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
</fx:root>
